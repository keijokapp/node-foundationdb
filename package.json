{
  "type": "module",
  "name": "@arbendium/foundationdb",
  "version": "0.0.0",
  "description": "Node.js bindings for the FoundationDB database",
  "keywords": [
    "foundationdb",
    "fdb",
    "database",
    "NoSQL",
    "ACID"
  ],
  "homepage": "https://foundationdb.org",
  "license": "MIT",
  "exports": "./lib/index.js",
  "files": [
    "binding.gyp",
    "lib",
    "prebuilds",
    "src"
  ],
  "scripts": {
    "install": "node-gyp-build",
    "test": "mocha -r ts-node/register test/*.ts",
    "prepublishOnly": "ls -ld prebuilds/darwin-arm64/foundationdb.node prebuilds/darwin-x64/foundationdb.node prebuilds/linux-x64/foundationdb.node",
    "prebuild": "prebuildify --napi --strip"
  },
  "dependencies": {
    "fdb-tuple": "^1.0.0",
    "node-gyp-build": "^4.8.1"
  },
  "devDependencies": {
    "@types/bindings": "^1.5.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.14.2",
    "@types/xml2js": "^0.4.14",
    "chalk": "^5.3.0",
    "mocha": "^10.4.0",
    "prebuildify": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "xml2js": "^0.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josephg/node-foundationdb.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "gypfile": true
}
